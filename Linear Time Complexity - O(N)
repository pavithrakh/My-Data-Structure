#include <stdio.h>

int main() {
    int arr[] = {10, 20, 30, 40, 50};
    int size = sizeof(arr) / sizeof(arr[0]);
    int target = 40;
    int found = 0;

    for (int i = 0; i < size; i++) {
        if (arr[i] == target) {
            printf("Target %d found at index %d\n", target, i);
            found = 1;
            break;
        }
    }
    if (!found) {
        printf("Target %d not found.\n", target);
    }
    return 0;
}

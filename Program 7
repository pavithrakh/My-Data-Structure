#include <stdio.h>
#include <stdlib.h>

int main() {
    int n1, n2, *a, *b, *c, i=0, j=0, k=0;
    scanf("%d%d", &n1, &n2);
    a = malloc(n1 * sizeof(int));
    b = malloc(n2 * sizeof(int));

    for (i = 0; i < n1; i++) scanf("%d", a+i);
    for (i = 0; i < n2; i++) scanf("%d", b+i);

    c = malloc((n1+n2)*sizeof(int));
    i=j=k=0;
    while (i<n1 && j<n2) {
        if (a[i] < b[j]) c[k++] = a[i++];
        else if (a[i] > b[j]) c[k++] = b[j++];
        else { c[k++] = a[i]; i++; j++; }
    }
    while (i<n1) c[k++] = a[i++];
    while (j<n2) c[k++] = b[j++];

    c = realloc(c, k * sizeof(int));

    for (i=0;i<k;i++) printf("%d ", c[i]);
    free(a); free(b); free(c);
    return 0;
}
